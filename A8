using System;

class Program
{
    static void Main()
    {
        string[] names = new string[10];

        for (int i = 0; i < 10; i++)
        {
            Console.Write("Enter name " + (i + 1) + ": ");
            names[i] = Console.ReadLine();
        }

        Console.WriteLine("\nNames in reverse order:");
        for (int i = 9; i >= 0; i--)
        {
            Console.WriteLine(names[i]);
        }
    }
}
