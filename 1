using System;

class CustomerAgeSurvey
{
    static void Main()
    {
        int[] ages = new int[5];
        int teenagerCount = 0;

        Console.WriteLine("Customer Age Survey");

        for (int i = 0; i < 5; i++)
        {
            int age;
            bool valid = false;

            // Loop until a valid age is entered
            do
            {
                Console.Write($"Enter age for customer {i + 1} (10-99): ");
                string input = Console.ReadLine();

                if (int.TryParse(input, out age))
                {
                    if (age >= 10 && age <= 99)
                    {
                        ages[i] = age;
                        valid = true;

                        // Check if it's a teenager
                        if (age >= 13 && age <= 19)
                        {
                            teenagerCount++;
                        }
                    }
                    else
                    {
                        Console.WriteLine("Invalid age. Age must be between 10 and 99.");
                    }
                }
                else
                {
                    Console.WriteLine("Invalid input. Please enter a number.");
                }

            } while (!valid);
        }

        // Output valid ages
        Console.WriteLine("\nList of valid ages:");
        foreach (int age in ages)
        {
            Console.WriteLine($"- {age}");
        }

        // Output teenager count
        Console.WriteLine($"\nNumber of teenagers (13â€“19): {teenagerCount}");
    }
}

