using System;

class Program
{
    static void Main()
    {
        // Array to store 20 lucky numbers from users
        int[] luckyNumbers = new int[20];

        // Predefined winning numbers
        int[] winningNumbers = { 7, 13, 21, 42, 55, 77 };

        Console.WriteLine("Enter your lucky numbers (between 1 and 99):");

        // Ask 20 users for their lucky numbers
        for (int i = 0; i < 20; i++)
        {
            int number;

            while (true)
            {
                Console.Write("User " + (i + 1) + ", enter your lucky number: ");
                string input = Console.ReadLine();

                // Try converting input to a number
                bool isValid = int.TryParse(input, out number);

                // Check if it's a valid number between 1 and 99
                if (isValid && number >= 1 && number <= 99)
                {
                    luckyNumbers[i] = number;
                    break; // exit the while loop
                }
                else
                {
                    Console.WriteLine("Invalid number! Please enter a number from 1 to 99.");
                }
            }

            // Check if the entered number is a winning number
            bool isWinner = false;
            for (int j = 0; j < winningNumbers.Length; j++)
            {
                if (luckyNumbers[i] == winningNumbers[j])
                {
                    isWinner = true;
                    break;
                }
            }

            // Congratulate the winner
            if (isWinner)
            {
                Console.WriteLine("ðŸŽ‰ Congratulations, User " + (i + 1) + "! Your number " + luckyNumbers[i] + " is a WINNING number!");
            }
            else
            {
                Console.WriteLine("Thank you, User " + (i + 1) + ". Better luck next time!");
            }

            Console.WriteLine(); // Blank line for neatness
        }

        Console.WriteLine("All users have entered their numbers. Good luck next time!");
    }
}
